{
  "name": "3drepo.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "./forever_app default",
    "stop": "forever stop 3drepo.js",
    "restart": "npm stop; npm start",
    "postinstall": "node ./postinstall.js",
    "upgrade": "git checkout master && git pull && npm restart",
    "test": "grunt test",
    "test-int-clean-up": "chmod 755 ./backend/test/cleanup/prompt.sh && ./backend/test/cleanup/prompt.sh && mongo localhost/admin ./backend/test/cleanup/db.js && NODE_ENV=test node ./backend/test/cleanup/queue.js && mongorestore ./backend/test/integrated/dump",
    "test-int": "NODE_ENV=test npm run create-ghost && grunt test-integrated",
    "dev": "nodemon 3drepo.js",
    "create-ghost": "node scripts/createGhost"
  },
