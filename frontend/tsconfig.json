{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "types": ["angular", "angular-material", "react", "react-dom", "lodash"],
    "lib": [
      "es5",
      "es6",
      "es2015",
      "dom",
    ],
    "target": "es5",
    "jsx": "react",
    "removeComments": true,
