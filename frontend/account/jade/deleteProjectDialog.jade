md-dialog.deleteDialog(aria-label="Delete Project")
	md-toolbar
		.md-toolbar-tools
			h2 {{vm.title}}
			span(flex)
			md-button.md-icon-button(ng-click="vm.closeDialog()")
				md-icon.angular-material-icons(aria-label="Close dialog") close
	md-content(layout-margin)
		div(layout="column", layout-align="start")
			div
				p
					| Do you really want to delete project
					=" "
					span#deleteName {{vm.projectDeleteName}}?
				br
				p#deleteWarning {{vm.deleteWarning}}
			md-input-container
				md-button.md-raised.md-accent(ng-click="vm.closeDialog()") No
				md-button.md-accent(ng-click="vm.deleteProject(vm.projectDeleteTeamspace, vm.projectDeleteName)") Yes
			p#deleteError(ng-if="vm.errorMessage") {{vm.errorMessage}}
