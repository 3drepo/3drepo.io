{
  "name": "3drepo.io-frontend",
  "version": "1.0.0",
  "description": "The frontend for 3drepo.io",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "watch": "webpack --watch --progress --config ./internals/webpack/webpack.dev.config.js",
    "build:dev": "webpack --progress --config ./internals/webpack/webpack.dev.config.js",
    "build": "webpack --progress --config ./internals/webpack/webpack.prod.config.js",
    "build:analyze": "webpack --progress --config ./internals/webpack/webpack.analyze.config.js",
    "wdm": "yarn run wdm:update && yarn run wdm:start",
    "wdm:start": "webdriver-manager start --standalone",
    "wdm:update": "webdriver-manager update --standalone",
    "e2e:ie": "protractor ./tests/e2e/built/ie/ie-config.js --specs ./tests/e2e/built/ie/ie.js",
    "e2e": "yarn run tsc:tests && protractor ./tests/e2e/built/config/config.js && yarn run e2e:ie",
    "e2e:one": "yarn run tsc:tests && yarn run protractor ./tests/e2e/built/config/config.js --specs ",
    "tsc:tests": "tsc --project ./tests",
    "unity-util": "webpack --config ./internals/webpack/webpack.unityutil.config.js",
    "lint:ts": "tslint --config tslint.json --project ./ --exclude tests --format stylish",
    "lint:css": "stylelint ./**/*.styles.ts",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:fix": "yarn lint:ts --fix",
    "docs": "typedoc --out ./docs ./components --options ./internals/typedoc.js --logger none"
  },
  "keywords": [
    "3drepo",
    "3drepo.io"
  ],
  "author": "3D Repo",
  "license": "AGPL-3.0",
