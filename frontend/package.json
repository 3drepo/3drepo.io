{
  "name": "3drepo.io-frontend",
  "version": "1.0.0",
  "description": "The frontend for 3drepo.io",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build:base:development": "webpack --watch --progress --config ./internals/webpack/webpack.dev.config.js",
    "build:base:production": "webpack --progress --config ./internals/webpack/webpack.prod.config.js",
    "watch": "yarn build:base:development --no-type-checking",
    "build": "yarn build:base:production --no-type-checking",
    "build:test": "yarn build:base:production",
    "build:dev": "yarn build --config ./internals/webpack/webpack.dev.config.js",
    "build:analyze": "yarn build --config ./internals/webpack/webpack.analyze.config.js",
    "wdm": "yarn run wdm:update && yarn run wdm:start",
    "wdm:start": "webdriver-manager start --standalone",
    "wdm:update": "webdriver-manager update --standalone",
    "e2e:ie": "protractor ./tests/e2e/built/ie/ie-config.js --specs ./tests/e2e/built/ie/ie.js",
    "e2e": "yarn run tsc:tests && protractor ./tests/e2e/built/config/config.js && yarn run e2e:ie",
    "e2e:one": "yarn run tsc:tests && yarn run protractor ./tests/e2e/built/config/config.js --specs ",
    "tsc:tests": "tsc --project ./tests",
    "unity-util": "webpack --config ./internals/webpack/webpack.unityutil.config.js",
    "lint:ts": "tslint --config tslint.json --project ./ --exclude tests --exclude internals --format stylish",
    "lint:css": "stylelint ./**/*.styles.ts",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:fix": "yarn lint:ts --fix",
    "docs": "typedoc --out ./docs ./globals --options ./internals/typedoc.js --logger none"
  },
  "keywords": [
    "3drepo",
    "3drepo.io"
  ],
  "author": "3D Repo",
  "license": "AGPL-3.0",
  "dependencies": {
    "@date-io/dayjs": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.10",
    "@fortawesome/free-solid-svg-icons": "5.6.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "axios": "0.19.0",
    "connected-react-router": "5.0.1",
    "copy-to-clipboard": "^3.2.0",
    "dayjs": "1.8.11",
    "eventemitter3": "3.1.0",
    "file-dialog": "0.0.7",
    "file-loader": "2.0.0",
    "filesize": "4.1.2",
    "font-awesome": "4.7.0",
    "formik": "1.5.7",
    "history": "4.9.0",
    "lodash": "4.17.15",
    "material-design-icons": "3.0.1",
    "material-ui-pickers": "2.2.4",
    "memoize-one": "5.0.5",
    "normalize.css": "8.0.1",
    "normalizr": "^3.4.0",
    "offline-plugin": "5.0.7",
    "query-string": "6.2.0",
    "react": "16.8.6",
    "react-autosuggest": "9.4.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.8.6",
    "react-dropzone": "6.0.2",
    "react-event-listener": "^0.6.6",
    "react-ga": "^2.5.7",
    "react-google-recaptcha": "1.0.5",
    "react-highlight-words": "0.14.0",
    "react-image": "2.0.0",
    "react-livechat": "1.1.1",
    "react-redux": "5.1.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-trello": "^2.2.3",
    "react-truncate": "2.4.0",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.2",
    "redux": "4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "0.16.0",
    "reduxsauce": "1.0.0",
    "reselect": "3.0.1",
    "semver-compare": "1.0.0",
    "simplebar": "4.0.0",
    "simplebar-react": "1.0.0",
    "socket.io-client": "2.0.4",
    "uuidv4": "^5.0.1",
    "worker-loader": "2.0.0",
    "yup": "0.26.10",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@types/lodash": "4.14.137",
    "@types/node": "8.5.2",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "autoprefixer": "9.3.1",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "extract-loader": "3.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "inquirer-directory": "2.1.0",
    "lodash-ts-imports-loader": "1.2.0",
    "postcss-loader": "3.0.0",
    "style-loader": "0.23.1",
    "styled-components": "4.3.2",
    "stylelint": "9.8.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.0",
    "ts-loader": "6.0.0",
    "tslint": "5.18.0",
    "tslint-react": "4.0.0",
    "typedoc": "0.13.0",
    "typescript": "3.4.3",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-livereload-plugin": "2.1.1",
    "webpack-stream": "4.0.0"
  },
  "resolutions": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "lodash": "4.17.15"
  }
}
