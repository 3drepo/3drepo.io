{   
    "configPath": "./",
    "extends": "tslint:recommended",
    "rules": {
        "indent": [true, "tabs"],
        "jsdoc-format": false,
        "prefer-for-of": false,
        "no-console": [true, "log", "warning"],
        "object-literal-sort-keys": false,
        "no-empty": false
    }
}