md-dialog#paypalDialog(aria-label="Paypal")
	md-toolbar
		.md-toolbar-tools
			h2 Subscription
			span(flex)
			md-button.md-icon-button(ng-click="vm.closeDialog()", ng-if="vm.closeDialogEnabled")
				md-icon.angular-material-icons(aria-label="Close dialog") close
	.userContent
		.cardInfo
			p(ng-if="vm.payPalError") {{vm.payPalError}} 
			br
			p(ng-if="!vm.payPalError") {{vm.payPalInfo}}
			div(flex layout="column" layout-align="center center" ng-if="!vm.closeDialogEnabled")
				br 
				br
				md-progress-circular()
			br
			div(ng-switch="vm.changeHelpToShow")
				p(ng-switch-when="98")
					| Please delete models to reduce quota.
