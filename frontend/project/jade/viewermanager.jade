div(ng-repeat="(name, viewer) in vm.viewers")
	viewer(
		node="node"
		name="{{viewer.name}}"
		account = "{{viewer.account}}"
		project = "{{viewer.project}}"
		branch = "{{viewer.branch}}"
		revision = "{{viewer.revision}}"
		manager = "vm.manager"
		event-service = "vm.vmservice"
		style="pointer-events:none"
	)
