div(layout-fill, layout="column", layout-align="center")
	div(layout="row", layout-align="center", flex="80")
		md-dialog(aria-label="Screen Shot", flex="85")
			md-toolbar
				.md-toolbar-tools
					h2 {{vm.title}}
					span(flex)
					md-button.md-icon-button(ng-click="vm.closeDialog()")
						md-icon.angular-material-icons(aria-label="Close dialog") close
			md-content(layout-margin)
