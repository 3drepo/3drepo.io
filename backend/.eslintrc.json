{
  "env": {
    "browser": true
  },
  "globals": {
    "console": false,
    "module": true,
    "document": false,
    "require": false,
    "Buffer": false,
    "describe": false,
    "it": false,
    "process": false
  },
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "no-var": 2,
    "strict": 0,
    "no-unused-vars": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2
  }
}
