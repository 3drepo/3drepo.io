{
  "name": "3drepo.io-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "./../run/forever_app default",
    "stop": "forever stop 3drepo.js",
    "restart": "yarn run stop; yarn run start",
    "upgrade": "git checkout master && git pull && yarn run restart",
    "create-ghost": "NODE_ENV=test NODE_CONFIG_DIR=../config node ../scripts/createGhost",
    "test": "yarn run test:unit && yarn run test:int",
    "test:coverage": "yarn test:int-clean && nyc yarn run test",
    "test:all": "yarn run test",
    "test:unit": "gulp test:unit",
    "test:int-clean": "./test/cleanup/cleanup.sh",
    "test:int": "gulp test:integrated",
    "test:int-one": "gulp test:integrated-one --script ",
    "test:clean-and-int": "yarn run test:int-clean && yarn run test:int",
    "test:clean-and-int-one": "yarn run test:int-clean && yarn run test:int-one",
    "test:add-mongo": "mongo --eval \"username='admintesting'; password='admintesting'\" ../scripts/mongo-admin.js"
  },
  "dependencies": {
    "amqplib": "0.4.1",
    "app-config": "0.1.5",
