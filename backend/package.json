{
  "name": "3drepo.io",
  "version": "3.0.0",
  "engines": {
    "node": "10.x.x"
  },
  "private": true,
  "scripts": {
    "start": "./../run/forever_app default",
    "stop": "forever stop 3drepo.js",
    "restart": "yarn run stop; yarn run start",
    "upgrade": "git checkout master && git pull && yarn run restart",
    "create-ghost": "NODE_ENV=test NODE_CONFIG_DIR=../config node ../scripts/createGhost",
    "gen-docs": "yarn run apidoc -i routes/ -o docs/ ",
    "test": "yarn run test:unit && yarn run test:int",
    "test:clean-db": "mongorestore --drop ../../tests/backend/dump",
    "test:coverage": "yarn test:int-clean && nyc yarn run test",
    "test:all": "yarn run test",
    "test:unit": "gulp test:unit",
    "test:unit-one": "gulp test:unit-one --script",
    "test:int-clean": "./test/cleanup/cleanup.sh",
    "test:int": "gulp test:integrated",
    "test:int-one": "gulp test:integrated-one --script ",
    "test:clean-and-int": "yarn run test:int-clean && yarn run test:int",
    "test:clean-and-int-one": "yarn run test:int-clean && yarn run test:int-one",
    "test:add-mongo": "mongo --eval \"username='admintesting'; password='admintesting'\" ../scripts/mongo-admin.js",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./"
  },
  "dependencies": {
    "amqplib": "0.5.3",
    "apidoc": "0.17.7",
    "app-config": "1.0.0",
    "archiver": "3.0.0",
    "async": "2.6.1",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connect-mongodb-session": "2.1.0",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "express-socket.io-session": "1.3.5",
    "fs.extra": "1.3.2",
    "html-pdf": "2.2.0",
    "json2csv": "4.3.3",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "moment-timezone": "0.5.23",
    "mongodb": "2.2.21",
    "mongoose": "4.7.6",
    "mongoose-double": "0.0.1",
    "multer": "1.4.1",
    "node-uuid": "1.4.8",
    "nodemailer": "5.1.1",
    "paypal-rest-sdk": "1.8.1",
    "pm2": "3.2.9",
    "pug": "2.0.3",
    "serialize-javascript": "1.6.1",
    "serve-favicon": "2.5.0",
    "sharp": "0.21.3",
    "shortid": "2.2.14",
    "soap": "0.25.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "vhost": "3.0.2",
    "winston": "2.2.0",
    "winston-daily-rotate-file": "3.6.0",
    "xml2js": "0.4.19",
    "xmlbuilder": "10.1.1",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-shallow-deep-equal": "1.4.6",
    "eslint": "5.13.0",
    "eslint-plugin-node": "8.0.1",
    "gulp": "4.0.0",
    "gulp-env": "0.4.0",
    "gulp-eslint": "5.0.0",
    "gulp-mocha": "6.0.0",
    "mocha": "5.2.0",
    "mockgoose": "6.0.3",
    "mongoose-mock": "0.4.0",
    "nyc": "13.2.0",
    "proxyquire": "2.1.0",
    "sinon": "7.2.3",
    "supertest": "3.4.2"
  },
  "description": "The backend for 3drepo.io",
  "main": "3drepo.js",
  "author": "3D Repo",
  "license": "GNU Affero General Public License v3.0"
}
