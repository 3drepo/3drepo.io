    "mongoose": "4.7.6",
    "mongoose-double": "0.0.1",
    "multer": "1.1.0",
    "node-uuid": "1.4.7",
    "nodemailer": "2.3.2",
    "paypal-rest-sdk": "1.7.1",
    "phantom": "4.0.5",
    "pug": "2.0.0-rc.4",
    "serialize-javascript": "1.4.0",
