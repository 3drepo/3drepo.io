{
  "name": "3drepo.io",
  "version": "4.17.0",
  "engines": {
    "node": "14.x.x"
  },
  "private": true,
  "scripts": {
    "gen-docs": "yarn apidoc -i routes/ -o docs/ ",
    "test": "yarn test:unit && yarn test:int",
    "test:cover": "cross-env NODE_ENV=test NODE_CONFIG_DIR=../config nyc yarn test ",
    "test:cover-one": "cross-env NODE_ENV=test NODE_CONFIG_DIR=../config nyc --reporter none mocha ",
    "test:coverall": "yarn test:cover && nyc report --reporter=text-lcov | coveralls",
    "test:unit": "cross-env NODE_ENV=test NODE_CONFIG_DIR=../config mocha test/unit --recursive --exit",
    "test:one": "cross-env NODE_ENV=test NODE_CONFIG_DIR=../config mocha --exit",
    "test:int-clean": "./test/cleanup/cleanup.sh",
    "test:int": "cross-env NODE_ENV=test NODE_CONFIG_DIR=../config mocha test/integrated --recursive --exit",
    "lint": "eslint ./ --rulesdir eslint-rules/",
    "lint:fix": "eslint --fix ./"
  },
