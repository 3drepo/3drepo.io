    "chai": "4.1.2",
    "eslint": "4.6.1",
    "eslint-plugin-node": "5.1.1",
    "gulp": "3.9.1",
    "gulp-env": "0.4.0",
    "gulp-eslint": "4.0.0",
    "gulp-mocha": "4.3.1",
    "mocha": "3.5.3",
